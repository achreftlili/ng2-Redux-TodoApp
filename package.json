{
  "name": "todo-redux",
  "version": "0.0.0",
  "description": "Todo example for ng2-redux",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=production node node_modules/.bin/webpack && cp ./index.html dist/index.html",
    "postinstall": "npm run build",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "dev": "webpack-dev-server -d --inline --progress --no-info --config webpack.dev.config.js --host 127.0.0.1",
    "start": "npm run server",

  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wbuchwalter/ng2-redux.git"
  },
  "keywords": [
    "angular",
    "redux",
    "ng2-redux",
    "webpack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wbuchwalter/ng2-redux/issues"
  },
  "homepage": "https://github.com/wbuchwalter/ng2-redux#readme",
  "dependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/http": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "core-js": "^2.3.0",
    "ng2-redux": "^4.0.0-beta.7",
    "redux": "^3.5.0",
    "redux-logger": "^2.6.1",
    "redux-localstorage": "^0.4.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.32",
    "@types/node": "^6.0.36",
    "awesome-typescript-loader": "^2.2.1",
    "cross-env": "^1.0.7",
    "node-libs-browser": "^0.5.2",
    "typescript": "2.0.2",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
